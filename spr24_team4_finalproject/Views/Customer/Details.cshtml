@model spr24_team4_finalproject.Models.AppUser

@{
    ViewData["Title"] = "Customer Details";
}

<h2>@ViewData["Title"]</h2>

<dl class="dl-horizontal">
    <dt>Email</dt>
    <dd>@Model.Email</dd>

    <dt>Name</dt>
    <dd>@Model.FullName</dd>

    <dt>Date of Birth:</dt>
    <dd>@Model.DateOfBirth</dd>

    <dt>Email:</dt>
    <dd>@Model.Email</dd>

    <dt>Address:</dt>
    <dd>@Model.Address</dd>

    <dt>Phone Number:</dt>
    <dd>@Model.PhoneNumber</dd>

    <dt>Advantage Number</dt>
    <dd>@Model.AdvNumber</dd>
</dl>

<a asp-controller="Customer" asp-action="Search" class="btn btn-secondary">Back to Search</a>
